{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "Speaker"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "external_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "blocks"
    },
    "role": {
      "type": "string"
    },
    "organization": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contact_email": {
      "type": "email"
    },
    "contact_phone": {
      "type": "string"
    },
    "contact_linkedin": {
      "type": "string"
    },
    "contact_instagram": {
      "type": "string"
    },
    "contact_office": {
      "type": "string"
    },
    "region": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "EMEA",
        "US"
      ]
    },
    "summit_sessions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::summit-session.summit-session",
      "inversedBy": "speakers"
    }
  }
}
