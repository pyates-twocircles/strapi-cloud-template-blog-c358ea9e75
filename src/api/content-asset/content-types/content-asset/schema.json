{
  "kind": "collectionType",
  "collectionName": "content_assets",
  "info": {
    "singularName": "content-asset",
    "pluralName": "content-assets",
    "displayName": "Content Asset"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "link",
        "file",
        "video"
      ]
    },
    "tags": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "file": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "region": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "EMEA",
        "US"
      ]
    },
    "summit_session": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::summit-session.summit-session",
      "inversedBy": "content_assets"
    }
  }
}
